S_0x11e92c0 .scope module, "TB" "TB" 2 4;
 .timescale -9 -10;
v0x121bdb0_0 .var "A", 31 0;
v0x121be90_0 .var "B", 31 0;
v0x121bf60_0 .var "clk", 0 0;
v0x121c060_0 .net "done", 0 0, v0x121b800_0;  1 drivers
v0x121c130_0 .var "reset", 0 0;
v0x121c1d0_0 .net "result", 31 0, v0x121b980_0;  1 drivers
v0x121c2a0_0 .var "start", 0 0;
S_0x11e9440 .scope module, "ff" "f" 2 18, 3 1 0, S_0x11e92c0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 1 "start"
    .port_info 3 /INPUT 32 "a"
    .port_info 4 /INPUT 32 "b"
    .port_info 5 /OUTPUT 32 "result"
    .port_info 6 /OUTPUT 1 "done"
v0x11ee520_0 .var "_a", 31 0;
v0x121b440_0 .var "_b", 31 0;
v0x121b520_0 .net "a", 31 0, v0x121bdb0_0;  1 drivers
