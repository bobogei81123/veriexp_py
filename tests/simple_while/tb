#! /usr/bin/vvp
:ivl_version "10.0 (stable)" "(v10_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 10;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xa48280 .scope module, "TB" "TB" 2 4;
 .timescale -9 -10;
v0xa812a0_0 .var "A", 31 0;
v0xa81380_0 .var "B", 31 0;
v0xa81450_0 .var "clk", 0 0;
v0xa81550_0 .net "done", 0 0, v0xa80b30_0;  1 drivers
v0xa81620_0 .var "reset", 0 0;
v0xa816c0_0 .net "result", 31 0, v0xa80d90_0;  1 drivers
v0xa81790_0 .var "start", 0 0;
S_0xa48400 .scope module, "ff" "f" 2 18, 3 1 0, S_0xa48280;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 1 "start"
    .port_info 3 /OUTPUT 32 "result"
    .port_info 4 /OUTPUT 1 "done"
    .port_info 5 /INPUT 32 "a"
    .port_info 6 /INPUT 32 "b"
v0xa4fbf0_0 .var "_a", 31 0;
v0xa80770_0 .var "_b", 31 0;
v0xa80850_0 .net "a", 31 0, v0xa812a0_0;  1 drivers
v0xa80940_0 .net "b", 31 0, v0xa81380_0;  1 drivers
v0xa80a20_0 .net "clk", 0 0, v0xa81450_0;  1 drivers
v0xa80b30_0 .var "done", 0 0;
v0xa80bf0_0 .var "res", 31 0;
v0xa80cd0_0 .net "reset", 0 0, v0xa81620_0;  1 drivers
v0xa80d90_0 .var "result", 31 0;
v0xa80f00_0 .net "start", 0 0, v0xa81790_0;  1 drivers
v0xa80fc0_0 .var "state", 31 0;
v0xa810a0_0 .var "t", 31 0;
E_0xa49920 .event posedge, v0xa80a20_0;
    .scope S_0xa48400;
T_0 ;
    %wait E_0xa49920;
    %load/vec4 v0xa80cd0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0xa80fc0_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0xa80d90_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0xa80b30_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0xa4fbf0_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0xa80770_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0xa810a0_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0xa80bf0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0xa80fc0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 32;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 32;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 32;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %dup/vec4;
    %pushi/vec4 6, 0, 32;
    %cmp/u;
    %jmp/1 T_0.6, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 32;
    %cmp/u;
    %jmp/1 T_0.7, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 32;
    %cmp/u;
    %jmp/1 T_0.8, 6;
    %dup/vec4;
    %pushi/vec4 9, 0, 32;
    %cmp/u;
    %jmp/1 T_0.9, 6;
    %jmp T_0.10;
T_0.2 ;
    %load/vec4 v0xa80f00_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.11, 8;
    %pushi/vec4 1, 0, 32;
    %jmp/1 T_0.12, 8;
T_0.11 ; End of true expr.
    %pushi/vec4 0, 0, 32;
    %jmp/0 T_0.12, 8;
 ; End of false expr.
    %blend;
T_0.12;
    %assign/vec4 v0xa80fc0_0, 0;
    %load/vec4 v0xa80f00_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.13, 8;
    %pushi/vec4 0, 0, 2;
    %jmp/1 T_0.14, 8;
T_0.13 ; End of true expr.
    %pushi/vec4 1, 0, 2;
    %jmp/0 T_0.14, 8;
 ; End of false expr.
    %blend;
T_0.14;
    %pad/s 1;
    %assign/vec4 v0xa80b30_0, 0;
    %jmp T_0.10;
T_0.3 ;
    %load/vec4 v0xa80850_0;
    %assign/vec4 v0xa4fbf0_0, 0;
    %load/vec4 v0xa80940_0;
    %assign/vec4 v0xa80770_0, 0;
    %pushi/vec4 4, 0, 32;
    %assign/vec4 v0xa80fc0_0, 0;
    %jmp T_0.10;
T_0.4 ;
    %load/vec4 v0xa80770_0;
    %assign/vec4 v0xa810a0_0, 0;
    %pushi/vec4 5, 0, 32;
    %assign/vec4 v0xa80fc0_0, 0;
    %jmp T_0.10;
T_0.5 ;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0xa80bf0_0, 0;
    %pushi/vec4 6, 0, 32;
    %assign/vec4 v0xa80fc0_0, 0;
    %jmp T_0.10;
T_0.6 ;
    %load/vec4 v0xa810a0_0;
    %cmpi/ne 0, 0, 32;
    %flag_mov 8, 4;
    %jmp/0 T_0.15, 8;
    %pushi/vec4 8, 0, 32;
    %jmp/1 T_0.16, 8;
T_0.15 ; End of true expr.
    %pushi/vec4 7, 0, 32;
    %jmp/0 T_0.16, 8;
 ; End of false expr.
    %blend;
T_0.16;
    %assign/vec4 v0xa80fc0_0, 0;
    %jmp T_0.10;
T_0.7 ;
    %load/vec4 v0xa80bf0_0;
    %assign/vec4 v0xa80d90_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0xa80b30_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0xa80fc0_0, 0;
    %jmp T_0.10;
T_0.8 ;
    %load/vec4 v0xa80bf0_0;
    %load/vec4 v0xa4fbf0_0;
    %add;
    %assign/vec4 v0xa80bf0_0, 0;
    %pushi/vec4 9, 0, 32;
    %assign/vec4 v0xa80fc0_0, 0;
    %jmp T_0.10;
T_0.9 ;
    %load/vec4 v0xa810a0_0;
    %subi 1, 0, 32;
    %assign/vec4 v0xa810a0_0, 0;
    %pushi/vec4 6, 0, 32;
    %assign/vec4 v0xa80fc0_0, 0;
    %jmp T_0.10;
T_0.10 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0xa48280;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "tb.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0xa48280 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0xa48280;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0xa81450_0;
    %inv;
    %store/vec4 v0xa81450_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0xa48280;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xa81450_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xa81790_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xa81620_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xa81620_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xa81620_0, 0, 1;
    %delay 30, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xa81790_0, 0, 1;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0xa812a0_0, 0, 32;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0xa81380_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xa81790_0, 0, 1;
    %delay 10, 0;
T_3.0 ;
    %load/vec4 v0xa81550_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz T_3.1, 8;
    %delay 10, 0;
    %jmp T_3.0;
T_3.1 ;
    %vpi_call 2 41 "$display", v0xa816c0_0 {0 0 0};
    %load/vec4 v0xa816c0_0;
    %cmpi/ne 2, 0, 32;
    %jmp/0xz  T_3.2, 4;
    %vpi_call 2 43 "$display", "error" {0 0 0};
    %vpi_call 2 44 "$finish" {0 0 0};
T_3.2 ;
    %delay 30, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xa81790_0, 0, 1;
    %pushi/vec4 7, 0, 32;
    %store/vec4 v0xa812a0_0, 0, 32;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0xa81380_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xa81790_0, 0, 1;
    %delay 10, 0;
T_3.4 ;
    %load/vec4 v0xa81550_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz T_3.5, 8;
    %delay 10, 0;
    %jmp T_3.4;
T_3.5 ;
    %vpi_call 2 57 "$display", v0xa816c0_0 {0 0 0};
    %load/vec4 v0xa816c0_0;
    %cmpi/ne 28, 0, 32;
    %jmp/0xz  T_3.6, 4;
    %vpi_call 2 59 "$display", "error" {0 0 0};
    %vpi_call 2 60 "$finish" {0 0 0};
T_3.6 ;
    %delay 30, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xa81790_0, 0, 1;
    %pushi/vec4 123, 0, 32;
    %store/vec4 v0xa812a0_0, 0, 32;
    %pushi/vec4 456, 0, 32;
    %store/vec4 v0xa81380_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
