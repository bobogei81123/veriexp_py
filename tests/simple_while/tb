T_0.2 ;
    %load/vec4 v0x213a750_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.11, 8;
    %pushi/vec4 1, 0, 32;
    %jmp/1 T_0.12, 8;
T_0.11 ; End of true expr.
    %pushi/vec4 0, 0, 32;
    %jmp/0 T_0.12, 8;
 ; End of false expr.
    %blend;
T_0.12;
    %assign/vec4 v0x213a810_0, 0;
    %jmp T_0.10;
T_0.3 ;
