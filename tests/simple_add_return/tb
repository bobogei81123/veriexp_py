#! /usr/bin/vvp
:ivl_version "10.0 (stable)" "(v10_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 10;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1f152e0 .scope module, "TB" "TB" 2 4;
 .timescale -9 -10;
v0x1f43910_0 .var "A", 31 0;
v0x1f439f0_0 .var "B", 31 0;
v0x1f43ac0_0 .var "clk", 0 0;
v0x1f43bc0_0 .var "reset", 0 0;
v0x1f43c90_0 .net "result", 31 0, v0x1f434e0_0;  1 drivers
v0x1f43d30_0 .var "start", 0 0;
S_0x1f15460 .scope module, "ff" "f" 2 17, 3 1 0, S_0x1f152e0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 1 "start"
    .port_info 3 /INPUT 32 "a"
    .port_info 4 /INPUT 32 "b"
    .port_info 5 /OUTPUT 32 "result"
    .port_info 6 /OUTPUT 1 "done"
v0x1f19680_0 .var "_a", 31 0;
v0x1f42fa0_0 .var "_b", 31 0;
v0x1f43080_0 .net "a", 31 0, v0x1f43910_0;  1 drivers
v0x1f43170_0 .net "b", 31 0, v0x1f439f0_0;  1 drivers
v0x1f43250_0 .net "clk", 0 0, v0x1f43ac0_0;  1 drivers
v0x1f43360_0 .var "done", 0 0;
v0x1f43420_0 .net "reset", 0 0, v0x1f43bc0_0;  1 drivers
v0x1f434e0_0 .var "result", 31 0;
v0x1f435c0_0 .net "start", 0 0, v0x1f43d30_0;  1 drivers
v0x1f43710_0 .var "state", 31 0;
E_0x1f16580 .event posedge, v0x1f43250_0;
    .scope S_0x1f15460;
T_0 ;
    %wait E_0x1f16580;
    %load/vec4 v0x1f43420_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x1f43710_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x1f43710_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 32;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 32;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x1f435c0_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.6, 8;
    %pushi/vec4 1, 0, 32;
    %jmp/1 T_0.7, 8;
T_0.6 ; End of true expr.
    %pushi/vec4 0, 0, 32;
    %jmp/0 T_0.7, 8;
 ; End of false expr.
    %blend;
T_0.7;
    %assign/vec4 v0x1f43710_0, 0;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x1f43080_0;
    %assign/vec4 v0x1f19680_0, 0;
    %load/vec4 v0x1f43170_0;
    %assign/vec4 v0x1f42fa0_0, 0;
    %pushi/vec4 2, 0, 32;
    %assign/vec4 v0x1f43710_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x1f43360_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x1f19680_0;
    %load/vec4 v0x1f42fa0_0;
    %add;
    %assign/vec4 v0x1f434e0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x1f43360_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x1f43710_0, 0;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1f152e0;
T_1 ;
    %vpi_call 2 13 "$dumpfile", "tb.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x1f152e0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x1f152e0;
T_2 ;
    %delay 10, 0;
    %load/vec4 v0x1f43ac0_0;
    %inv;
    %store/vec4 v0x1f43ac0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x1f152e0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1f43ac0_0, 0, 1;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x1f43910_0, 0, 32;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0x1f439f0_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1f43d30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1f43bc0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1f43bc0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1f43bc0_0, 0, 1;
    %delay 30, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1f43d30_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1f43d30_0, 0, 1;
    %delay 40, 0;
    %vpi_call 2 36 "$display", v0x1f43c90_0 {0 0 0};
    %load/vec4 v0x1f43c90_0;
    %cmpi/ne 30, 0, 32;
    %jmp/0xz  T_3.0, 4;
    %vpi_call 2 38 "$display", "error" {0 0 0};
T_3.0 ;
    %vpi_call 2 40 "$finish" {0 0 0};
    %end;
    .thread T_3;
